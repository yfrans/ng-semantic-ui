{
  "name": "ng-semantic-ui",
  "version": "0.6.2",
  "description": "Angular directives for Semantic-UI library",
  "scripts": {
    "pack": "browserify index.js -o ./dist/ng-semantic-ui.js",
    "annotate": "ng-annotate -a ./dist/ng-semantic-ui.js -o ./dist/ng-semantic-ui.js",
    "uglify": "uglifyjs ./dist/ng-semantic-ui.js -o ./dist/ng-semantic-ui.min.js --compress --mangle --screw-ie8",
    "build": "npm run pack && npm run annotate && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yfrans/ng-semantic-ui.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "semantic-ui",
    "semanticui",
    "directive"
  ],
  "author": "Yossi Frances",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yfrans/ng-semantic-ui/issues"
  },
  "homepage": "https://github.com/yfrans/ng-semantic-ui#readme",
  "devDependencies": {
    "browserify": "^13.0.1",
    "express": "^4.14.0",
    "ng-annotate": "^1.2.1"
  }
}
